<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sh.pj.pet.PetMapper">


	<select id="getAll" resultType="com.sh.pj.pet.PetDTO">
		select*from petsitter_tbl
	</select>

	<select id="detail" resultType="com.sh.pj.pet.PetDTO">
		select*from petsitter_tbl where ps_no = #{ps_no}
	</select>

	<insert id="regDolbom">
		insert into dolbom_tbl values (dolbom_tbl_seq.nextval, #{d_id}, #{d_name}, #{d_category}, #{d_year}, #{d_month}, #{d_day}, #{d_hour}, #{d_check}, #{d_location}, #{d_gender}, #{d_personality}, #{d_pay}, #{d_cctv}, #{d_act}, #{d_sitterage}, #{d_need}, #{d_title}, #{d_term})
	</insert>


</mapper>